    <!-- /wp:paragraph --></div>
    <!-- /wp:column -->
    
    <!-- wp:column {"verticalAlignment":"center","className":"border-roxo"} -->
    <div class="wp-block-column is-vertically-aligned-center border-roxo"><!-- wp:heading {"textAlign":"center","level":3,"style":{"typography":{"fontStyle":"normal","fontWeight":"500","fontSize":"30px"}},"textColor":"black"} -->
