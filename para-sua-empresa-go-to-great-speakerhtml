    <h2 class="has-text-align-center has-black-color has-text-color has-x-large-font-size" style="font-style:normal;font-weight:600;margin-bottom:1em">Diferenciais</h2>
    <!-- /wp:heading -->
    
    <!-- wp:columns {"verticalAlignment":"center","align":"wide"} -->
    <div class="wp-block-columns alignwide are-vertically-aligned-center"><!-- wp:column {"verticalAlignment":"center","className":"border-azul","layout":{"inherit":false}} -->
    <div class="wp-block-column is-vertically-aligned-center border-azul"><!-- wp:heading {"textAlign":"center","level":3,"style":{"typography":{"fontStyle":"normal","fontWeight":"500","fontSize":"30px"}},"textColor":"black"} -->
    <h3 class="has-text-align-center has-black-color has-text-color" style="font-size:30px;font-style:normal;font-weight:500">Inglês focado<br>em negócios</h3>
    <!-- /wp:heading -->
    
    <!-- wp:paragraph {"align":"center","style":{"typography":{"fontSize":"18px"}},"textColor":"black"} -->
